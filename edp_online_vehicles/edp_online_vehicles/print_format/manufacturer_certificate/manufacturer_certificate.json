 "html": "{% set model_data = frappe.call('edp_online_vehicles.events.get_model_data.get_model_data', modelname=doc.model) %}\r\n\r\n<div style=\"display: flex; justify-content: space-between; align-items: center; margin-bottom: 5px;\">\r\n    <div style=\"text-align: left; margin-left: 50px;\">\r\n        <img src=\"/files/220812 Mahindra Rise CMYK Logo-01.png\" alt=\"Mahindra Letter Head\" height=\"35.0\" style=\"height: 35.0px;\">\r\n    </div>\r\n    <div style=\"text-align: left; font-size: 10px; margin-right: 70px;\">\r\n        <p style=\"margin: 0;\">\r\n            Mahindra & Mahindra South Africa Pty. Ltd.<br>\r\n            Southdowns Ridge Office Park, Block A, 1st Floor<br>\r\n            CNR John Vorster and Nellmapius Drive, Southdowns<br>\r\n            Centurion 0169<br>\r\n            Tel- +27 12 661 3161<br>\r\n            <a href=\"https://www.mahindra.co.za\">www.mahindra.co.za</a>\r\n        </p>\r\n    </div>\r\n</div>\r\n\r\n<br><br>\r\n\r\n<div class=\"print-heading\">\r\n    <h2 style=\"text-align: center; margin: 0;\">Vehicle Manufacturers Certificate</h2>\r\n</div>\r\n\r\n<table  style=\"width:100%\">\r\n  <tr>\r\n    <th>Chassis Number</th>\r\n    <th>Engine Number</th>\r\n    <th>Colour</th>\r\n  </tr>\r\n  <tr>\r\n    <td>{{ doc.name }}</td>\r\n    <td>{{ doc.engine_no }}</td>\r\n    <td>{{ doc.colour }}</td>\r\n  </tr>\r\n</table>\r\n\r\n<div style=\"font-family: Arial, sans-serif; font-size: 12px; margin: 20px;\">\r\n\r\n    <!-- Data Table Section -->\r\n    <table style=\"width: 100%; border-collapse: collapse;\">\r\n        <tbody>\r\n            <tr>\r\n                <td style=\"width: 30%; font-weight: bold;\">Natis Released</td>\r\n                <td>{{ doc.natis_release_date or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Vehicle Register Number</td>\r\n                <td>{{ doc.register_no or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Natis Control Number</td>\r\n                <td>{{ doc.control_no or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Make</td>\r\n                <td>{{ doc.brand or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Model Name</td>\r\n                <td>{{ doc.model or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Model Derivative</td>\r\n                <td>{{ model_data.derivitive or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Tare (T)</td>\r\n                <td>{{ model_data.tare or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Engine Capacity</td>\r\n                <td>{{ model_data.engine_capacity or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Fuel Type</td>\r\n                <td>{{ model_data.fuel_type or \"\" }}  (01 - Petrol; 02 - Diesel)</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">GVM</td>\r\n                <td>{{ model_data.gvm or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">GCM</td>\r\n                <td>{{ model_data.gcm or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Engine Power</td>\r\n                <td>{{ model_data.net_power or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">CO2</td>\r\n                <td>{{ model_data.co2_emission or \"\" }} g/km</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">Model Code</td>\r\n                <td>{{ model_data.model_code_common or \"\" }}</td>\r\n            </tr>\r\n            <tr>\r\n                <td style=\"font-weight: bold;\">MM Code</td>\r\n                <td>{{ model_data.mm_code or \"\" }}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n\r\n    <!-- Footer Section -->\r\n    <div style=\"margin-top: 40px; font-size: 10px; text-align: left;\">\r\n        <p style=\"margin: 0;\">Postal Address: PO Box 69079, Highveld Park Centurion, Pretoria, 0169, South Africa<br>\r\n            Mahindra and Mahindra South Africa (Proprietary) Limited<br>\r\n            Directors: Mr V Nakra*, Mr R Goyal*, Mr R Gupta*, Mr S Kulkarni* (*Indian Citizen)</p>\r\n            <br>\r\n        <p style=\"margin-top: 10px; text-align: center;\">Copyright \u00a9 2022 Mahindra & Mahindra Ltd. All rights reserved</p>\r\n    </div>\r\n</div>",
